<com.ly.framework.ui.WindowLayerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/head_line_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context="com.ly.justsoso.headline.HeadLineFragment">

    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <android.support.v4.view.PagerTabStrip
            android:id="@+id/pager_tab_strip_list"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:gravity="center"></android.support.v4.view.PagerTabStrip>
    </android.support.v4.view.ViewPager>

</com.ly.framework.ui.WindowLayerLayout>
